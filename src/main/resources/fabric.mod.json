{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",
  "name": "Create Mob Eggs",
  "description": "Makes mobs drop spawn-eggs in crushing wheels if they have certain effects. Regular mobs - regeneration, undead mobs - weakness.",
  "authors": [
    "8KV"
  ],
  "contact": {
    "sources": "https://github.com/Fabricators-of-Create/create-fabric-addon-template"
  },
  "license": "CC0",
  "icon": "assets/modid/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.modid.ExampleMod"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
